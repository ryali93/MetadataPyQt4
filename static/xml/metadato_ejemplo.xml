          <gmd:title>
            <gco:CharacterString>Base de datos geoquímica de suelos-Subcuencas Linga y Bajo Tambo</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:DateTime>2008-06-23T12:53:00</gco:DateTime>
              </gmd:date>
              <gmd:dateType>
